<script>//<![CDATA[
/* main.js */
window.onload = function() {
    $('[data-src]').each(function() {
        var $this = $(this);
        var $url = $(this).attr('data-src');
		$this.attr('src', $url);
        $('<img src="'+$url+'"/>').on('load', function(){
            $this.removeAttr('data-src');
    	});
    });
    $('[data-bg]').each(function() {
        var $url = $(this).attr('data-bg');
        $(this).css('background-image', 'url('+$url+')').removeAttr('data-bg');
    });
    $('#socmed a, #custom a').each(function() {
        var $domain = '//' + $(this).attr('href').split('/')[2];
		$domain = $domain.replace('//www.','').replace('//m.','').replace('//api.','').replace('//web.','').replace('//','');
        $('img', this).attr('src', 'https://www.google.com/s2/favicons?domain=' + $domain + '&sz=32');
    });
    $('#highlight').each(function() {
        var amount = 120;
        $(this).bind("DOMMouseScroll mousewheel", function(event) {
            var oEvent = event.originalEvent,
                direction = oEvent.detail ? oEvent.detail * -amount : oEvent.wheelDelta,
                position = $(this).scrollLeft();
            position += direction > 0 ? -amount : amount;
            $(this).scrollLeft(position);
            event.preventDefault();
        });
    });
}
 //]]></script>

